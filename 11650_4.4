N = int(input())
coord = []
for _ in range(N):
    (x, y) = map(int, input().split())
    coord.append((x, y))
sorted_coord = sorted(coord, key = lambda x:(x[0], x[1]))
for x, y in sorted_coord:
    print(x, y)

import sys
N = int(sys.stdin.readline())
coord = []
for _ in range(N):
    x, y = map(int, sys.stdin.readline().split())
    coord.append((x, y))
sorted_coord = sorted(coord)
for x, y in sorted_coord:
    print(x, y)
