#직접 설계한 코드
A, B = map(int, input().split())
divisor_A = []
divisor_B = []
for i in range(1, A+1):
    if A%i == 0:
        divisor_A.append(i)
for j in range(1, B+1):
    if B%j == 0:
        divisor_B.append(j)
cd = set(divisor_A).intersection(set(divisor_B))
max_cd = max(cd)
multiple_A = [k*A for k in range(1, B+1)]
multiple_B = [l*B for l in range(1, A+1)]
cm = list(set(multiple_A).intersection(set(multiple_B)))
min_cm = min(cm)
print(max_cd)
print(min_cm)

#가장 깔끔한 코드
import math
A, B = map(int, input().split())
gcd = math.gcd(A, B)
lcm = A * B // gcd
print(gcd)
print(lcm)
