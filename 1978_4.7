N = int(input())
numbers = list(map(int,input().split()))
count_of_prime = 0
for i in numbers:
    if i == 1:
        count_of_prime += 0
    if i == 2:
        count_of_prime += 1
    if i > 2:
        is_prime = True
        for n in range(2,i//2+1):
            if i%n == 0:
                is_prime = False
                break
        if is_prime == True:
            count_of_prime += 1
        else:
            count_of_prime += 0
print(count_of_prime)

N = int(input())
numbers = list(map(int, input().split()))
max_num = max(numbers)
is_prime = [True] * (max_num + 1)
is_prime[0] = is_prime[1] = False
for i in range(2, int(max_num ** 0.5) + 1):
    if is_prime[i]:
        for j in range(i * i, max_num + 1, i):
            is_prime[j] = False
count_of_prime = sum(1 for num in numbers if is_prime[num])
print(count_of_prime)
